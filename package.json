{
  "name": "backend-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test-cov": "LOG=no jest --coverage --runInBand --forceExit --detectOpenHandles",
    "dev": "npm run clean && ts-node src/index.ts | pino-pretty",
    "dev:watch": "npm run clean && nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/index.ts\" | pino-pretty",
    "start": "npm run clean && ts-node src/index.ts",
    "build": "tsc --build tsconfig.json",
    "test": "tsc --build tsconfig.json; jest --coverage --runInBand --forceExit --detectOpenHandles",
    "test:quick": "jest",
    "clean": "find src/ -type f -name \"*.js\" -delete",
    "watch": "nodemon . | pino-pretty",
    "build:watch": "tsc --build -w tsconfig.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@knax/objection-soft-delete": "^1.0.8",
    "@koa/cors": "^3.1.0",
    "axios": "^0.21.1",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.23.0",
    "dotenv": "^9.0.1",
    "escape-json-node": "^2.0.0",
    "excel4node": "^1.7.2",
    "fastest-validator": "^1.10.1",
    "fs-extra": "^10.0.0",
    "knex": "^0.95.4",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-jwt": "^4.0.1",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "mailchimp-api-v3": "^1.15.0",
    "midtrans-client": "^1.2.4",
    "mysql": "^2.18.1",
    "objection": "^2.2.15",
    "on-finished": "^2.3.0",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "pino-pretty": "^4.8.0",
    "ts-md5": "^1.2.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.3",
    "@types/koa-router": "^7.4.2",
    "@types/lodash": "^4.14.172",
    "@types/luxon": "^1.26.5",
    "@types/node": "^16.7.1",
    "@types/on-finished": "^2.3.1",
    "@types/pino": "^6.3.8",
    "@types/uuid": "^8.3.0",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.0",
    "ts-loader": "^9.2.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/services/**/*",
      "!src/modules/index.js"
    ],
    "testEnvironment": "node"
  }
}
